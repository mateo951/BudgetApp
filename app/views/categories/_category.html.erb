    <div class="flex-row">
        <p>
          <%= image_tag "#{category.icon}" %>
        </p>
      <p class="title-category">
        <%= category.name %>
      </p>
    </div>
    <p>
      <%= '$140' %>
    </p>
  </div>
</div>
